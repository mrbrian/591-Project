	@test -d .tmp/project1.0.0 || mkdir -p .tmp/project1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/project1.0.0/ && $(COPY_FILE) --parents algebra.h material.h polyroots.h scene.h shapes.h lodepng/lodepng.h photon.h misc.h camera.h kdtree/kdtree.h .tmp/project1.0.0/ && $(COPY_FILE) --parents algebra.cpp main.cpp material.cpp polyroots.cpp scene.cpp lodepng/lodepng.cpp photon.cpp camera.cpp misc.cpp .tmp/project1.0.0/ && (cd `dirname .tmp/project1.0.0` && $(TAR) project1.0.0.tar project1.0.0 && $(COMPRESS) project1.0.0.tar) && $(MOVE) `dirname .tmp/project1.0.0`/project1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/project1.0.0


