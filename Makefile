####### Compile

algebra.o: algebra.cpp algebra.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o algebra.o algebra.cpp

main.o: main.cpp lodepng/lodepng.h \
		algebra.h \
		scene.h \
		shapes.h \
		material.h \
		polyroots.h \
		misc.h \
		photon.h \
		camera.h \
		kdtree/kdtree.h \
		kdtree/kdtree.hxx
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

material.o: material.cpp material.h \
		algebra.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o material.o material.cpp

polyroots.o: polyroots.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o polyroots.o polyroots.cpp

scene.o: scene.cpp scene.h \
		shapes.h \
		algebra.h \
		material.h \
		polyroots.h \
		misc.h \
		lodepng/lodepng.h \
		photon.h \
		camera.h \
		kdtree/kdtree.h \
		kdtree/kdtree.hxx
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o scene.o scene.cpp

lodepng.o: lodepng/lodepng.cpp lodepng/lodepng.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o lodepng.o lodepng/lodepng.cpp

photon.o: photon.cpp photon.h \
		algebra.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o photon.o photon.cpp

camera.o: camera.cpp camera.h \
		algebra.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o camera.o camera.cpp

misc.o: misc.cpp misc.h \
		lodepng/lodepng.h \
		algebra.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o misc.o misc.cpp

